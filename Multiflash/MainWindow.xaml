<Window x:Class="JBlam.Multiflash.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JBlam.Multiflash"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <local:MultiflashViewModel />
    </Window.DataContext>
    <DockPanel AllowDrop="True" DragEnter="DockPanel_DragEnter" Drop="DockPanel_Drop">
        <local:ComPortSelector DockPanel.Dock="Top" DataContext="{Binding ComPortSelector}" />
        <Button DockPanel.Dock="Top" Click="Button_Click">Go</Button>
        <Label Name="label1" FontFamily="Consolas">asfd</Label>
        <local:StreamingConsole x:Name="console"></local:StreamingConsole>
    </DockPanel>
</Window>
